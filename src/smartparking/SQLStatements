 ##examples:
 CREATE TABLE IF NOT EXISTS `t_Orders` (`id` INTEGER AUTO_INCREMENT , `name` VARCHAR(255) NOT NULL , `idNo` VARCHAR(255) , `plateNumber` VARCHAR(255) , `gender` TINYINT(1) , `BirthString` VARCHAR(255) , `regDate` VARCHAR(50) , `email` VARCHAR(255) , `phone` VARCHAR(255) , `howActivate` INTEGER , `IPLevel` INTEGER ,  UNIQUE (`name`),  UNIQUE (`idNo`),  UNIQUE (`plateNumber`),  UNIQUE (`email`),  UNIQUE (`phone`), PRIMARY KEY (`id`) ) ENGINE=InnoDB
 ##
 CREATE TABLE IF NOT EXISTS `t_parkingLots` (`id` INTEGER AUTO_INCREMENT , `name` VARCHAR(255) NOT NULL , `address` VARCHAR(255) NOT NULL , `totalSpaces` INTEGER NOT NULL , `availableSpaces` INTEGER NOT NULL , `hourlyPrice` DOUBLE PRECISION NOT NULL , `openingHours` VARCHAR(255) , `notice` VARCHAR(255) , PRIMARY KEY (`id`) ) ENGINE=InnoDB
 ##
 CREATE TABLE IF NOT EXISTS `t_orders` (`id` INTEGER AUTO_INCREMENT , `id_lot` INTEGER NOT NULL , `id_Order` INTEGER NOT NULL , `date` VARCHAR(50) NOT NULL , `amount` DOUBLE PRECISION NOT NULL , PRIMARY KEY (`id`) ) ENGINE=InnoDB
 ##
CREATE INDEX `t_Orders_email_idx` ON `t_Orders` ( `email` )
##add constraints:
alter table t_orders add foreign key(id_user) references t_users(id);
alter table t_orders add foreign key(id_lot) references t_parkingLots(id);
create unique index idx_bolb_username on t_users(name(120))
